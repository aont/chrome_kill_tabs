{
    "name": "suspend",
    "version": "0.1.3",
    "manifest_version": 2,
    "description": "suspend",
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "browser_action": {
        "default_icon": "action_icon.png",
        "default_title": "suspend tabs"
    },
    "background": {
        "scripts": ["background.js"]
    }
}
